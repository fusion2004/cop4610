diff -rupN linux-2.6.36/alexis_jefferson/alexis_jefferson.c linux-2.6.36-dev/alexis_jefferson/alexis_jefferson.c
--- linux-2.6.36/alexis_jefferson/alexis_jefferson.c	1969-12-31 19:00:00.000000000 -0500
+++ linux-2.6.36-dev/alexis_jefferson/alexis_jefferson.c	2011-09-13 11:07:40.000000000 -0400
@@ -0,0 +1,57 @@
+#include <linux/linkage.h>
+#include <linux/time.h>
+#include <linux/sched.h>
+
+asmlinkage long sys_alexis_jefferson(int pantherid)
+{
+	struct tm result_time;
+	struct timespec kernel_time;
+	int seconds, minutes, hours, month, day;
+	long year;
+	char am_pm;
+
+	/* gets the time in a struct tm */
+	kernel_time = current_kernel_time();
+	time_to_tm(kernel_time.tv_sec, 0, &result_time);
+
+	/* extracts the time of interest and puts it into human readable format */
+	seconds = result_time.tm_sec;
+	minutes = result_time.tm_min;
+
+	/* It will be AM unless otherwise noted. */
+	am_pm = 'a';
+
+	/* Subtracting 4 for the timezone. */
+	hours = result_time.tm_hour - 4;
+
+	/* If the hours are negative, then add 12 to make it a positive time. */
+	if(hours < 0)
+		hours = hours + 12;
+
+	/* Turning it non-military time */
+	if(hours > 12)
+	{
+		hours = hours - 12;
+		am_pm = 'p';
+	}
+
+	if(hours == 0)
+		hours = 12;
+
+	month = result_time.tm_mon + 1;
+	day = result_time.tm_mday;
+	year = result_time.tm_year + 1900;
+
+	
+	
+
+	printk("sys_alexis_jefferson called from process %d with panther ID, %d.\n",
+		current->pid, pantherid);
+
+	
+	printk("Current time is: %d:%02d:%02d%cm, %d/%d/%ld.\n", hours, minutes, seconds,
+		am_pm, month, day, year);
+
+
+	return 0;
+}
